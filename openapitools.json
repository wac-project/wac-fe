{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.12.0",
    "generators": {
      "ambulance": {
        "generatorName": "typescript-fetch"   ,
        "glob": "api/ambulance.openapi.yaml",
        "output": "#{cwd}/src/api/ambulance",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
